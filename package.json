{
  "name": "ctf-game",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/crisu83/ctf-game.svg?branch=master)](https://travis-ci.org/crisu83/ctf-game) [![Code Climate](https://codeclimate.com/github/crisu83/ctf-game/badges/gpa.svg)](https://codeclimate.com/github/crisu83/ctf-game) [![GitHub version](https://badge.fury.io/gh/crisu83%2Fctf-game.svg)](https://badge.fury.io/gh/crisu83%2Fctf-game)",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack -p",
    "build:server": "babel src --ignore src/client --out-dir dist --source-maps --minified",
    "clean:dist": "rm -r dist/*",
    "start": "npm run build && node dist/index.js",
    "start:client": "webpack -d --progress --colors --hot --watch",
    "start:server": "nodemon --exec babel-node -- src/index.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "mocha --compilers js:babel-core/register --require test/helper.js --recursive",
    "test:client:watch": "npm run test:client -- --watch",
    "test:server": "mocha --compilers js:babel-core/register --require test/helper.js --recursive",
    "test:server:watch": "npm run test:server -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crisu83/ctf-game.git"
  },
  "keywords": [],
  "author": "Christoffer Niska <christofferniska@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crisu83/ctf-game/issues"
  },
  "homepage": "https://github.com/crisu83/ctf-game#readme",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chance": "^0.8.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "express": "^4.10.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "foundation-sites": "^6.1.2",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "sass-loader": "^3.1.2",
    "scss-loader": "0.0.1",
    "shortid": "^2.2.4",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-livereload-plugin": "^0.5.3",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      [
        "babel-plugin-module-alias",
        [
          {
            "src": "./src/shared",
            "expose": "shared"
          }
        ]
      ]
    ]
  }
}
